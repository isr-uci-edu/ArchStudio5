<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.archstudio.myxgen.brick">
      <brick
            class="org.archstudio.xarchadt.variability.core.XArchADTVariabilityMyxComponent"
            id="org.archstudio.xarchadt.variability.core.XArchADTVariabilityMyxComponent"
            name="XArch ADT Variability Impl">
         <interface
               class="org.archstudio.xarchadt.IXArchADTFileListener"
               delegate="events"
               direction="out"
               domain="bottom"
               getter="true"
               id="fileEvents"
               name="fileEvents"
               single="false">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADTModelListener"
               delegate="events"
               direction="out"
               domain="bottom"
               getter="true"
               id="modelEvents"
               name="modelEvents"
               single="false">
         </interface>
         <interface
               class="org.archstudio.xarchadt.variability.IXArchADTVariabilityListener"
               delegate="events"
               direction="out"
               domain="bottom"
               getter="true"
               id="variabilityEvents"
               name="variabilityEvents"
               single="false">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADT"
               delegate="variable"
               direction="in"
               domain="bottom"
               getter="true"
               id="xarch"
               name="xarch"
               single="true">
         </interface>
      </brick>
   </extension>

</plugin>
